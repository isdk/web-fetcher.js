{
  "title": "Should restore session state from options",
  "actions": [
    {
      "action": "goto",
      "params": {
        "url": "/"
      }
    }
  ],
  "options": {
    "forceSessionState": true,
    "sessionState": {
      "sessions": [
        {
          "id": "restored-session",
          "cookieJar": {
            "version": "tough-cookie@4.1.2",
            "storeType": "MemoryCookieStore",
            "rejectPublicSuffixes": true,
            "cookies": [
              {
                "key": "restored-cookie",
                "value": "value-from-state",
                "domain": "localhost",
                "path": "/",
                "hostOnly": true,
                "creation": "2024-01-01T01:00:00.000Z",
                "lastAccessed": "2024-01-01T02:00:00.000Z"
              }
            ]
          },
          "userData": {},
          "maxErrorScore": 3,
          "errorScore": 0,
          "usageCount": 0,
          "createdAt": "2024-01-01T01:00:00.000Z",
          "expiresAt": "2024-01-01T02:00:00.000Z"
        }
      ]
    }
  },
  "expected": {
    "statusCode": 200,
    "cookies": {
      "contains": "restored-cookie"
    }
  }
}
