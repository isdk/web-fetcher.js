{
  "title": "should use firefox for camoufox-js to bypass bot detection when antibot is enabled",
  "engine": "playwright",
  "options": {
    "antibot": true
  },
  "actions": [
    { "action": "goto", "args": ["/"] },
    {
      "action": "extract",
      "args": [{
        "type": "string",
        "selector": "#results"
      }]
    }
  ],
  "expected": {
    "html": {
      "and": [
        { "contains": "\"webdriver\":false" },
        { "contains": "firefox", "caseInsensitive": true }
      ]
    }
  }
}