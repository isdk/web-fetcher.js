{
  "title": "should use firefox for camoufox-js to bypass bot detection when antibot is enabled",
  "engine": "playwright",
  "options": {
    "antibot": true
  },
  "actions": [
    {
      "action": "goto",
      "params": {
        "url": "/"
      }
    },
    {
      "action": "extract",
      "params": {
        "type": "string",
        "selector": "#results"
      }
    }
  ],
  "expected": {
    "html": {
      "and": [
        {
          "contains": "\"webdriver\":false"
        },
        {
          "contains": "firefox",
          "caseInsensitive": true
        }
      ]
    }
  }
}
