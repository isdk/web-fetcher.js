{
  "title": "should handle page navigation by clicking a link",
  "actions": [
    {
      "action": "goto",
      "params": {
        "url": "/"
      }
    },
    {
      "action": "click",
      "params": {
        "selector": "a[href=\"/page2\"]"
      }
    }
  ],
  "expected": {
    "finalUrl": "/page2",
    "html": {
      "contains": "<h2>Page 2</h2>"
    }
  }
}
