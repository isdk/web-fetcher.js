{
  "title": "waitFor should not throw when failOnTimeout is false",
  "engine": "playwright",
  "options": {
    "timeoutMs": 100
  },
  "actions": [
    {
      "action": "goto",
      "params": {
        "url": "/"
      }
    },
    {
      "action": "waitFor",
      "params": {
        "selector": "#non-existent",
        "failOnTimeout": false
      }
    },
    {
      "action": "extract",
      "params": {
        "selector": "h1",
        "type": "string"
      }
    }
  ],
  "expected": {
    "statusCode": 200,
    "data": "Hello World"
  }
}
