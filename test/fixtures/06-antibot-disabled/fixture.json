{
  "title": "should not use firefox by default when antibot is disabled",
  "engine": "playwright",
  "options": {
    "antibot": false
  },
  "actions": [
    {
      "action": "goto",
      "params": {
        "url": "/"
      }
    },
    {
      "action": "extract",
      "params": {
        "type": "string",
        "selector": "#results"
      }
    }
  ],
  "expected": {
    "html": [
      {
        "contains": "\"webdriver\":false"
      },
      {
        "not": {
          "contains": "firefox",
          "caseInsensitive": true
        }
      }
    ]
  }
}
